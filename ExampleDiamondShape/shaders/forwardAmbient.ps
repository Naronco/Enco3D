#version 120

varying vec3 texCoord;

uniform vec3 mat_diffuseColor;
uniform sampler2D mat_diffuseTexture;
uniform vec3 rendering_globalAmbientLightColor;

void main()
{
	gl_FragColor = texture2D(mat_diffuseTexture, texCoord.xy) * vec4(mat_diffuseColor, 1.0f) * vec4(rendering_globalAmbientLightColor, 1.0f);
}